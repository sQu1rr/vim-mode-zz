{
  "name": "vim-mode-zz",
  "main": "./lib/vim-mode-zz",
  "version": "0.1.1",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 4
    },
    "line_endings": {
      "value": "unix",
      "level": "error"
    }
  },
  "description": "Allows to close opened tabs with Vim's ZZ command",
  "keywords": [
    "vim",
    "vim-mode",
    "ZZ"
  ],
  "activationCommands": {
    "atom-workspace": "vim-mode-zz:saveAndClose"
  },
  "repository": "https://github.com/sQu1rr/vim-mode-zz",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "consumedServices": {
    "vim-mode": {
      "versions": {
        "^0.1.0": "consumeVim"
      }
    }
  }
}
